{
  "id": "https://schema.management.azure.com/schemas/2016-01-01/Microsoft.AzureBridge.Admin.json#",
  "title": "Microsoft.AzureBridge.Admin",
  "description": "Microsoft AzureBridge.Admin Resource Types",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "resourceDefinitions": {
    "activations": {
      "description": "Microsoft.AzureBridge.Admin/activations",
      "properties": {
        "apiVersion": {
          "enum": [
            "2016-01-01"
          ],
          "type": "string"
        },
        "azureRegistrationResourceIdentifier": {
          "description": "Azure registration resource identifier.",
          "type": "string"
        },
        "displayName": {
          "description": "Name displayed for the product.",
          "type": "string"
        },
        "expiration": {
          "description": "The activation expiration.",
          "type": "string"
        },
        "marketplaceSyndicationEnabled": {
          "description": "Value indicating whether the marketplace syndication feature is enabled.",
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "name": {
          "description": "Name of the activation.",
          "type": "string"
        },
        "provisioningState": {
          "description": "Provisioning state of the resource.",
          "oneOf": [
            {
              "enum": [
                "Stopped",
                "Starting",
                "Running",
                "Stopping",
                "Succeeded",
                "Downloading"
              ],
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "resources": {
          "items": {
            "oneOf": [
              {
                "$ref": "#/definitions/activations_downloadedProducts_childResource"
              }
            ]
          },
          "type": "array"
        },
        "type": {
          "enum": [
            "Microsoft.AzureBridge.Admin/activations"
          ],
          "type": "string"
        },
        "usageReportingEnabled": {
          "description": "Value indicating whether the usage reporting feature is enabled.",
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        }
      },
      "required": [
        "name",
        "apiVersion",
        "type"
      ],
      "type": "object"
    },
    "activations_downloadedProducts": {
      "description": "Microsoft.AzureBridge.Admin/activations/downloadedProducts",
      "properties": {
        "apiVersion": {
          "enum": [
            "2016-01-01"
          ],
          "type": "string"
        },
        "id": {
          "description": "URI of the resource.",
          "type": "string"
        },
        "name": {
          "description": "Name of the product.",
          "type": "string"
        },
        "properties": {
          "description": "Properties of the resource.",
          "oneOf": [
            {
              "$ref": "#/definitions/DownloadedProduct"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "Microsoft.AzureBridge.Admin/activations/downloadedProducts"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    }
  },
  "definitions": {
    "Compatibility": {
      "description": "Product compatibility",
      "properties": {
        "description": {
          "description": "Full error message if any compatibility issues are found",
          "type": "string"
        },
        "isCompatible": {
          "description": "Tells if product is compatible with current device",
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "issues": {
          "description": "List of all issues found",
          "oneOf": [
            {
              "items": {
                "enum": [
                  "HigherDeviceVersionRequired",
                  "LowerDeviceVersionRequired",
                  "CapacityBillingModelRequired",
                  "PayAsYouGoBillingModelRequired",
                  "DevelopmentBillingModelRequired",
                  "AzureADIdentitySystemRequired",
                  "ADFSIdentitySystemRequired",
                  "ConnectionToInternetRequired",
                  "ConnectionToAzureRequired",
                  "DisconnectedEnvironmentRequired"
                ],
                "type": "string"
              },
              "type": "array"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "message": {
          "description": "Short error message if any compatibility issues are found",
          "type": "string"
        }
      },
      "type": "object"
    },
    "DownloadedProduct": {
      "description": "Properties for aggregate usage.",
      "properties": {
        "billingPartNumber": {
          "description": "Billing part number.",
          "type": "string"
        },
        "compatibility": {
          "description": "Product compatibility with current device.",
          "oneOf": [
            {
              "$ref": "#/definitions/Compatibility"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "description": {
          "description": "Description of the product.",
          "type": "string"
        },
        "displayName": {
          "description": "Name displayed for the product.",
          "type": "string"
        },
        "galleryItemIdentity": {
          "description": "Gallery item identity.",
          "type": "string"
        },
        "galleryPackageBlobSasUri": {
          "description": "The URI to the .azpkg file that provides information required for showing product in the gallery.",
          "type": "string"
        },
        "iconUris": {
          "description": "Information about gallery icons.",
          "oneOf": [
            {
              "$ref": "#/definitions/GalleryIcons"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "legalTerms": {
          "description": "Legal terms for the product.",
          "type": "string"
        },
        "links": {
          "description": "Gallery item identity.",
          "oneOf": [
            {
              "items": {
                "$ref": "#/definitions/ProductLink"
              },
              "type": "array"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "offer": {
          "description": "Offer name.",
          "type": "string"
        },
        "offerVersion": {
          "description": "Offer version.",
          "type": "string"
        },
        "payloadLength": {
          "description": "Size in bytes.",
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "privacyPolicy": {
          "description": "Privacy policy of the product.",
          "type": "string"
        },
        "productKind": {
          "description": "The kind. E.g. VirtualMachineProductProperties.ProductKind or WebApp, SolutionTemplate.",
          "type": "string"
        },
        "productProperties": {
          "description": "the product properties. At the moment only VirtualMachineProductProperties is allowed.",
          "oneOf": [
            {
              "$ref": "#/definitions/ProductProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "provisioningState": {
          "description": "Provisioning state of the resource.",
          "oneOf": [
            {
              "enum": [
                "Stopped",
                "Starting",
                "Running",
                "Stopping",
                "Succeeded",
                "Downloading"
              ],
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "publisherDisplayName": {
          "description": "Name of publisher.",
          "type": "string"
        },
        "publisherIdentifier": {
          "description": "Publisher identifier.",
          "type": "string"
        },
        "sku": {
          "description": "Product SKU.",
          "type": "string"
        },
        "vmExtensionType": {
          "description": "Extension type of the VM.",
          "type": "string"
        }
      },
      "type": "object"
    },
    "GalleryIcons": {
      "description": "Icons for the gallery items.",
      "properties": {
        "hero": {
          "description": "The hero (815x290) icon relative path.",
          "type": "string"
        },
        "large": {
          "description": "The large (115x115) icon relative path.",
          "type": "string"
        },
        "medium": {
          "description": "The medium (90x90) icon relative path.",
          "type": "string"
        },
        "small": {
          "description": "The small (40x40) icon relative path.",
          "type": "string"
        },
        "wide": {
          "description": "The wide (255x115) icon relative path.",
          "type": "string"
        }
      },
      "type": "object"
    },
    "ProductLink": {
      "description": "Link to a product.",
      "properties": {
        "displayName": {
          "description": "Displayed name of product.",
          "type": "string"
        },
        "uri": {
          "description": "URI to product.",
          "type": "string"
        }
      },
      "type": "object"
    },
    "ProductProperties": {
      "description": "Additional properties of the product",
      "properties": {
        "version": {
          "description": "The version of the product",
          "type": "string"
        }
      },
      "type": "object"
    },
    "activations_downloadedProducts_childResource": {
      "description": "Microsoft.AzureBridge.Admin/activations/downloadedProducts",
      "properties": {
        "apiVersion": {
          "enum": [
            "2016-01-01"
          ],
          "type": "string"
        },
        "id": {
          "description": "URI of the resource.",
          "type": "string"
        },
        "name": {
          "description": "Name of the product.",
          "type": "string"
        },
        "properties": {
          "description": "Properties of the resource.",
          "oneOf": [
            {
              "$ref": "#/definitions/DownloadedProduct"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "downloadedProducts"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    }
  }
}